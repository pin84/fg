.df {
  display: flex;
  align-items: center;
  justify-content: center;
}
#home {
  box-sizing: border-box;
  padding: 40rpx;
}
#home .list {
  display: flex;
  flex-wrap: wrap;
}
#home .list .item {
  padding: 10rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
  height: 140rpx;
  border: 1px solid #ccc;
  position: relative;
  border-radius: 10rpx;
  margin: 5rpx;
}
#home .list .item .close {
  position: absolute;
  right: 0;
  top: 0;
  padding: 5rpx;
  color: #ccc;
}
#home .list .item .uploadIcon {
  font-size: 96rpx;
  color: #ccc;
  font-weight: 100;
}
#home .list .item .image {
  width: 100%;
  height: 100%;
}
#home .textareaBox {
  margin: 32rpx 0;
  width: 100%;
  border: 2rpx solid #EEEEEE;
  border-radius: 8rpx;
  padding: 16rpx;
  box-sizing: border-box;
}
#home .btn {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 12rpx;
  color: white;
  background: #e64340;
  font-size: 32rpx;
  margin: 100rpx auto;
}
#home .btn:active {
  opacity: 0.8;
}
#home .text {
  color: red;
  font-size: 24rpx;
  margin: 24rpx 0;
}
#home .com-input {
  border: 1px solid #f1f1f1;
  padding: 10rpx 0;
  font-size: 28rpx;
}
#home .picker-box {
  display: flex;
  align-items: center;
  width: 100%;
}
#home .picker-box .picker {
  width: 100%;
  padding: 5rpx 0;
}
#home .picker-box .addr-box {
  width: 100%;
}
#home .picker-box .iconfont {
  flex: 0 0 30rpx;
}
#home .addr-text {
  height: 160rpx;
}
.cell_item {
  padding: 0;
  min-height: 88rpx;
}
.cell_item .item_right {
  color: #1F3332;
}
.cell_item .item_right .lineInput {
  text-align: right;
}
.cell_item_last {
  margin-bottom: 32rpx;
}
